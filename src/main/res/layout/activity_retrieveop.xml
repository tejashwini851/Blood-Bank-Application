        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2" />

    <TextView
        android:id="@+id/genderop"
        android:layout_width="182dp"
        android:layout_height="33dp"
        app:layout_constraintBottom_toBottomOf="parent"
